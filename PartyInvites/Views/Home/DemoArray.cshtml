@using PartyInvites.Models
@model PartyInvites.Models.MyProduct[]

@{
    ViewBag.Title = "DemoArray";
    Layout = "~/Views/_BasicLayout.cshtml";
}

@if (Model.Length > 0)
{
    <table>
        <thead>
        <tr><td>Product</td><td>Price</td></tr>
        </thead>
        <tbody>
        @foreach (MyProduct p in Model)
        {
            <tr>
                <td>@p.Name</td>
                <td>@p.Price</td>
            </tr>
        }

        </tbody>
    </table>
}
else
{
    <h2>No product data</h2>
}
